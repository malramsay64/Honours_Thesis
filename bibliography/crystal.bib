 doi = {http://dx.doi.org/10.1016/j.physrep.2014.11.004},
 issn = {0370-1573},
 journal = {Phys. Rep.},
 keywords = {Model glassforming systems},
 note = {The role of local structure in dynamical arrest},
 number = {0},
 pages = {1 - 75},
 title = {The role of local structure in dynamical arrest},
 url = {http://www.sciencedirect.com/science/article/pii/S0370157314004062},
 volume = {560},
 year = {2015}
}

