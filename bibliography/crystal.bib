 doi = {10.1016/j.ejps.2006.04.016},
 issn = {0928-0987},
 journal = {Eur. J. Pharm. Sci.},
 keywords = {Lipid formulation classification system (LFCS)},
 note = {Advances in Understanding Oral Absorption and Delivery of Problem Compounds Selected Papers from the 3rd World Conference on Drug Absorption, Transport and Delivery},
 number = {3--4},
 pages = {278 - 287},
 title = {Formulation of poorly water-soluble drugs for oral administration: Physicochemical and physiological issues and the lipid formulation classification system},
 url = {http://www.sciencedirect.com/science/article/pii/S0928098706001151},
 volume = {29},
