	Keywords = {Entropy; Enthalpy; Fusion; Organic; Walden},
	Month = {11},
	Number = {1--2},
	Pages = {131--142},
	Title = {Entropy--enthalpy compensation in the fusion of organic molecules: implications for Walden's rule and molecular freedom in the liquid state},
	Ty = {JOUR},
