 date-modified = {2015-01-28 00:15:12 +0000},
 journal = {EPL},
 number = {4},
 pages = {289},
 title = {Strong and fragile liquids in percolation approach to the glass transition},
 url = {http://stacks.iop.org/0295-5075/35/i=4/a=289},
 volume = {35},
 year = {1996}
}

