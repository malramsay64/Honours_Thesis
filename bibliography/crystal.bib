@article{zeng:11,
	Annote = {10.1126/science.1200324},
	Author = {Zeng, Qiaoshi and Sheng, Hongwei and Ding, Yang and Wang, Lin and Yang, Wenge and Jiang, Jian-Zhong and Mao, Wendy L. and Mao, Ho-Kwang},
	Date = {2011/06/17},
	Date-Added = {2015-01-14 05:43:00 +0000},
