 day = {15},
 doi = {10.1016/S0040-6031(99)00301-9},
 isbn = {0040-6031},
 journal = {Thermochim. Acta},
 keywords = {Entropy; Enthalpy; Fusion; Organic; Walden},
 month = {11},
